IDI_ICON1  ICON    DISCARDABLE     "resources/DBLParse.ico"

VS_VERSION_INFO VERSIONINFO
    FILEVERSION    ${CMAKE_PROJECT_VERSION_MAJOR},${CMAKE_PROJECT_VERSION_MINOR},${CMAKE_PROJECT_VERSION_PATCH},${CMAKE_PROJECT_VERSION_TWEAK}
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "CompanyName", "${CMAKE_PROJECT_AUTHOR}"
            VALUE "FileDescription", "${CMAKE_PROJECT_DESCRIPTION}"
            VALUE "FileVersion", "${CMAKE_PROJECT_VERSION}"
            VALUE "LegalCopyright", "Copyright (C) 2021 ${CMAKE_PROJECT_AUTHOR}"
            VALUE "InternalName", "${CMAKE_PROJECT_NAME}"
            VALUE "OriginalFilename", "${CMAKE_PROJECT_NAME}"
            VALUE "ProductName", "${CMAKE_PROJECT_NAME}"
            VALUE "ProductVersion", "${CMAKE_PROJECT_VERSION}"
        END
    END
END